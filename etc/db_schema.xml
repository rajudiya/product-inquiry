<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="Pwc_ProductInquiry_Custom_Userform" resource="default" engine="innodb" comment="pwc_productInquiry_custom_userform Table">
		<column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="Pwc ProductInquiry User Id"/>
		<column name="fname" nullable="true" xsi:type="text" comment="Customer First Name"/>
		<column name="lname" nullable="true" xsi:type="text" comment="Customer Last Name"/>
		<column name="email" nullable="true" xsi:type="text" comment="Customer Email Id"/>
		<column name="address" nullable="true" xsi:type="varchar" comment="Customer Address"/>
		<column name="inquiry_subject" nullable="true" xsi:type="varchar" comment="Customer ProductInquiry Subject"/>
		<column name="message" nullable="true" xsi:type="varchar" comment="Customer ProductInquiry message"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="id"/>
		</constraint>
	</table>
</schema>